{
  "manifest_version": 3,
  "name": "Rate My Professor Helper",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "storage",
    "nativeMessaging",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.ratemyprofessors.com/*",
    "http://*.edu/*",
    "https://*.edu/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.edu/*",
        "https://*.edu/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}