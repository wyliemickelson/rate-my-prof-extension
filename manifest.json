{
  "manifest_version": 3,
  "name": "Rate My Professor Helper",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello.png"
  },
  "content_scripts": [
    {

      "matches": ["https://*.edu/*"],
      "js": ["index.js"],
      "run_at": "document_idle"
    }
  ]
}